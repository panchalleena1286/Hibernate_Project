<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.domain.Employee" table="EmployeTable">
		<id name="empNo" column="EMPLOYEE_NO" length="4">
			<generator class="assigned"></generator>
		</id>
		<property name="empName" column="EMPLOYEE_NAME" length="25"></property>
	</class>
</hibernate-mapping>